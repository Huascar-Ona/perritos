<div class="container">
  <%= simple_form_for @photo, :html => { :multipart => true, :class => "dropzone"  } do |f| %>
    <%= f.input :dog_id, as: :hidden, input_html: { value: @dogId} %>
    <div class="fallback">  
      <%= f.file_field :image, multiple: true %>
      <%= f.button :submit, class: "btn btn-sm btn-primary center-block" %>
    </div>

  <% end %>

  <div class="index">
  </div>
  <%= link_to 'Cancelar', @dog, class: 'btn btn-danger' %>
</div>